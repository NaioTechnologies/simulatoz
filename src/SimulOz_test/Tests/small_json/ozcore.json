    "tracks": [ { "d" : 0.65 } , { "d" : 0.0 } ],

    "uturns": [ { "d" : 0.60 } ],

    "two_pass": false,
    "crop_gap": 0.10,

    "uturn_direction": 1,
    "track_side" : 1
  }
}

